# Git Aliases
alias gp="git push"
alias gpo="git push -u origin"
alias gc="git commit -m"
alias gst="git status"
alias gd="git diff" 
alias gco="git checkout"

# Base Aliases
alias localhost="cd /var/www/html/"
alias chphp="sudo update-alternatives --config php"
alias definer="sed -i 's/DEFINER=[^*]*\*/\*/g'"
alias dump-help="echo 'mysqldump --single-transaction -h /host/ -u /user/ -p /database/ > /file/.sql'"

# Magento-Cloud Aliases
alias dump="db:dump"
alias get-media="mount:download --mount=pub/media --target=pub/media"

# Magento 2 Aliases
alias bm="bin/magento"
alias cf="cache:flush"
alias su="setup:upgrade"
alias di="setup:di:compile"
alias cleanall="bin/magento setup:upgrade && bin/magento c:f && rm -r var/* generated/*"

